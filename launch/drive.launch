<launch>
  <param name="robot_description" textfile="$(find ranger_description)/ranger.urdf" />
  <node pkg="ranger_ros" type="ros_interface" name="ranger_ros"/>
  <node pkg="rviz" type="rviz" name="rviz" output="screen"/>
  <node pkg="differential_drive" type="diff_tf.py" name="diff_tf">
     <rosparam param="rate">8.0</rosparam>
     <rosparam param="ticks_meter">69049</rosparam>
     <rosparam param="base_width">0.396</rosparam>
  </node>
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" /> 
</launch>
